{
    "importerClass": "LaravelEnso\\Examples\\app\\Imports\\Importers\\UserGroupImporter",
    "entryLimit": 5000,
    "stopsOnIssues": false,
    "sheets": [{
        "name": "groups",
        "columns": [{
            "name": "name",
            "laravelValidations": "string|required|unique:user_groups,name",
            "complexValidations": "unique_in_column"
        }, {
            "name": "description",
            "laravelValidations": "string|nullable"
        }]
    }]
}